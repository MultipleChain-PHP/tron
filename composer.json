{
    "name": "multiplechain/tron",
    "version": "0.1.0",
    "description": "MultipleChain Tron",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "MultipleChain\\Tron\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MultipleChain\\Tron\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "phpunit",
        "phpcs": "phpcs --standard=phpcs.xml .",
        "phpcbf": "phpcbf --standard=phpcs.xml .",
        "coverage": "phpunit --coverage-clover=coverage/clover.xml",
        "phpstan": "phpstan analyse --memory-limit=2G --configuration=phpstan.neon"
    },
    "authors": [
        {
            "name": "Halil Beycan",
            "email": "halilbeycan0@gmail.com"
        }
    ],
    "require": {
        "multiplechain/utils": "^0.1.3",
        "multiplechain/definitions": "^0.1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5",
        "phpstan/phpstan": "^1.11",
        "squizlabs/php_codesniffer": "^3.10",
        "slevomat/coding-standard": "^8.15",
        "phpunit/php-code-coverage": "^10.1"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
